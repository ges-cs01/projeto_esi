<h1>Listar transações</h1>

<table>
  <thead>
    <tr>
      <th>Descrição</th>
      <th>Valor</th>
      <th>Ação</th>
    </tr>
  </thead>

  <tbody>
    <% @transacoes.each do |transacao| %>
      <tr>
        <td>
          <%= transacao.descricao %>
        </td>
        <td>
          <%= transacao.valor %>
        </td>
        <td>
          <button>Detalhes</button>
        </td>
      </tr>
    <% end %>
  </tbody>

</table>
